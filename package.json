{
  "name": "odogwu",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.18",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.4.0",
    "flowbite": "^1.6.5",
    "flowbite-react": "^0.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "swiper": "^9.3.2",
    "validator": "^13.9.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "axios-mock-adapter": "^1.21.4",
    "msw": "^1.2.2",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2"
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "collectCoverageFrom": [
      "src/{!(App),}.js",
      "src/{!(Index),}.js",
      "src/{!(Sliding),}.jsx",
      "src/{!(View),}.js",
      "mocks/{!(mockFetch),}.js",
      "mocks/{!(handlers),}.js"
    ]
  }
}
